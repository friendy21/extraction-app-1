    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/jest": "^29.5.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/extraction-app-refactored.git"
  },
