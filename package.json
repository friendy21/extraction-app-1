    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "framer-motion": "^12.7.3",
    "lucide-react": "^0.487.0",
    "motion": "^12.7.3",
    "next": "15.3.0",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.3",
