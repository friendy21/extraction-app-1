{
  "name": "extraction-app-refactored",
  "version": "2.0.0",
  "description": "Refactored extraction application with enhanced database schema and comprehensive API layer",
  "private": true,
  "keywords": [
    "nextjs",
    "typescript",
    "data-extraction",
    "employee-management",
    "organizational-data",
    "api-client",
    "dashboard"
  ],
  "author": "Manus AI",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run type-check && npm run lint",
    "clean": "rm -rf .next out dist",
    "export": "next export",
    "analyze": "ANALYZE=true npm run build",
    "docs:api": "echo 'API documentation available in API_DOCUMENTATION.md'",
    "migration:check": "echo 'Please refer to the migration guide in API_DOCUMENTATION.md'"
  },
